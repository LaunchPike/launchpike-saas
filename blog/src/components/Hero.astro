---
import HeroSection from "./Hero/HeroSection/HeroSection";
const {
  title = "Launch your SaaS MVP",
  compare = {
    launchpike: {
      time: 90,
      span: "minutes",
    },
    others: {
      time: 3,
      span: "days",
    },
  },
} = Astro.props;
---

<HeroSection client:load title={title} compare={compare} />
